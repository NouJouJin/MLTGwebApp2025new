# ソースマップ生成の設定（本番環境では false を推奨.）
GENERATE_SOURCEMAP=false

# Moralis API Key
# 取得方法: https://moralis.io/ でアカウントを作成し、API Keyを取得
REACT_APP_MORALIS_API_KEY=your_moralis_api_key_here

# ThirdWeb Client ID
# 取得方法: https://thirdweb.com/ でアカウントを作成し、Client IDを取得
REACT_APP_THIRDWEB_CLIENT_ID=your_thirdweb_client_id_here

# Airtable API Key
# 取得方法: https://airtable.com/create/tokens でPersonal Access Tokenを作成
REACT_APP_AIRTABLE_API_KEY=your_airtable_api_key_here
